  use_frameworks!



  # Pods for TheCampusApp

	pod 'Firebase'
	pod 'Firebase/Core'
	pod 'Firebase/Auth'
	pod 'Firebase/Database'


  target 'TheCampusAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TheCampusAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
